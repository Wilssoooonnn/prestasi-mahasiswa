                                <?php
                                // Menampilkan data dari table tingkat_kompetisi ke dalam option
                                $tingkat = $kompetisiModel->getTingkatKomp();
                                if (!empty($tingkat)):
                                    foreach ($tingkat as $t): ?>
                                        <option value="<?= htmlspecialchars($t['id']); ?>">
                                            <?= htmlspecialchars($t['nama']); ?>
                                        </option>
                                    <?php endforeach; ?>
